<%- model_class = User -%>
<h1 class="text-white">Clientes - <%= link_to 'Cadastrar Cliente',
            "/users/sign_up",
            :class => 'btn btn-primary' %></h1>

  <% @users.each do |user| %>
    <div class="col-lg-4 box text-black" align="center">
      <img class="rounded-circle" src="icon-users0.png" alt="Icone Documento" width="100" height="100">
      <h2><%= user.name %></h2>
      <p><%= user.email %></p>

    </div>
  <% end %>
<br><br>
